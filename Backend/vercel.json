{
  "version": 2, 
  "builds": [
    { "src": "./index.js", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" },
    { "src": "/articles", "dest": "/index.js" },
    { "src": "/articles/delete", "dest": "/index.js" },
    { "src": "/description", "dest": "/index.js" }
  ]

}